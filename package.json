{
  "name": "aws-e2e-tests",
  "version": "1.0.0",
  "description": "An example setup for AWS e2e tests.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist && rimraf cdk/dist",
    "build": "npm run clean && npm run webpack && npm run cdk:build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc",
    "tsc:cdk": "tsc -p cdk/tsconfig.json",
    "tsc:watch": "tsc -w",
    "test:e2e": "npm run tsc:cdk && NODE_ENV=test mocha -r ts-node/register cdk/test/acceptance/*.spec.ts",
    "cdk:build": "tsc -p cdk/tsconfig.json",
    "cdk:diff": "cdk diff",
    "cdk:deploy": "cdk deploy",
    "webpack": "tsc && webpack --config webpack.common.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rinkkasatiainen/aws-e2e-tests.git"
  },
  "keywords": [
    "aws",
    "serverless",
    "testing",
    "end-2-end",
    "testing"
  ],
  "author": "Aki Salmi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rinkkasatiainen/aws-e2e-tests/issues"
  },
  "homepage": "https://github.com/rinkkasatiainen/aws-e2e-tests#readme",
  "devDependencies": {
    "@aws-cdk/aws-apigateway": "^1.31.0",
    "@aws-cdk/aws-dynamodb": "^1.31.0",
    "@aws-cdk/aws-iam": "^1.31.0",
    "@aws-cdk/aws-lambda": "^1.31.0",
    "@aws-cdk/aws-lambda-event-sources": "^1.31.0",
    "@aws-cdk/aws-sns": "^1.31.0",
    "@aws-cdk/core": "^1.31.0",
    "@types/aws-lambda": "^8.10.47",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "approvals": "^3.0.5",
    "aws-sdk": "^2.653.0",
    "cdk": "^1.31.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "mocha": "^7.1.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "uuid": "^7.0.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "moment": "^2.24.0"
  }
}
